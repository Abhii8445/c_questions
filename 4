#include <stdio.h>

int main()
{
    int a,b;
    printf("enter the divident number -");
    scanf("%d",&a);
    printf("enter the divisor number -");
    scanf("%d",&b);
    if (a%b==0){
        printf(" %d is completely divisible by %d",a,b);
    }
    else{
        printf("is  not completely divisible");
    }

    return 0;
}
